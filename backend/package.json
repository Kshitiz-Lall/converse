{
  "name": "devtoolkit-backend",
  "version": "1.0.0",
  "description": "Backend services for Developer Toolkit application",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon src/index.ts",
    "appmap": "npx appmap-node nodemon --exec ts-node src/index.ts",
    "build": "tsc",
    "lint": "eslint src/**/*.ts",
    "poller:build": "tsc src/pollers/llm-leaderboard-poller.ts --outDir dist/utils",
    "poller:run": "node dist/pollers/llm-leaderboard-poller.js",
    "poller:dev": "ts-node src/pollers/llm-leaderboard-poller.ts",
    "poller:start": "npm run poller:build && npm run poller:run",
    "poller:watch": "nodemon --watch src/pollers/llm-leaderboard-poller.ts --exec npm run poller:dev"
  },
  "dependencies": {
    "@types/cheerio": "^1.0.0",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/redis": "^4.0.11",
    "axios": "^1.8.3",
    "bcryptjs": "^3.0.2",
    "cheerio": "^1.1.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "ioredis": "^5.6.0",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.13.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "openai": "^5.19.1",
    "redis": "^4.7.0",
    "sharp": "^0.33.5",
    "uuid": "^11.1.0",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "@types/compression": "^1.7.3",
    "@types/cors": "^2.8.15",
    "@types/express": "^4.17.20",
    "@types/ioredis": "^5.0.0",
    "@types/js-yaml": "^4.0.8",
    "@types/morgan": "^1.9.7",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.8.10",
    "@types/sharp": "^0.32.0",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "eslint": "^9.35.0",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  }
}
